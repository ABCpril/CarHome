<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.carba.web.mapper.SeriesMapper">

    <select id="findseriesById" resultType="com.carba.web.pojo.Series">
        SELECT *
        FROM series
        WHERE series_id = #{seriesId}
    </select>
</mapper>